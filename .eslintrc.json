{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "require-jsdoc" : "off",
        "quotes": [
            "error",
            "double"
        ],
        "indent": [
            "warn",
            2
        ],
        // "no-console": "warn",
        "no-empty": "warn",
        "max-len": "warn"
    }
}